# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Security Configuration
ENCRYPTION_KEY=your-32-character-encryption-key-here
JWT_SECRET=your-jwt-secret-for-audit-tokens
IP_SALT=your-ip-hashing-salt

# Email Configuration (for contact forms)
CONTACT_EMAIL=contact@spinezone-sandiego.com
FROM_EMAIL=noreply@spinezone-sandiego.com
SENDGRID_API_KEY=your-sendgrid-api-key

# Environment
NODE_ENV=production

# Optional: Google verification
GOOGLE_VERIFICATION_CODE=your-google-verification-code

# Optional: Analytics
GOOGLE_ANALYTICS_ID=your-ga-id

# Google Maps API (Required for interactive maps)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key