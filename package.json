{
  "name": "spinezone-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:profile": "next build --profile",
    "postbuild": "npm run sitemap:generate && npm run bundle-report",
    "sitemap:generate": "next-sitemap --config next-sitemap.config.js || echo 'Warning: Sitemap generation failed, continuing...'",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "ANALYZE=true npm run build",
    "preview": "npx serve out",
    "bundle-report": "npx @next/bundle-analyzer",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output=html --output-path=./performance-reports/lighthouse",
    "perf:webpagetest": "echo 'Run WebPageTest manually at https://www.webpagetest.org/'",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.2.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.54.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.316.0",
    "next": "14.1.0",
    "next-sitemap": "^4.2.3",
    "rate-limiter-flexible": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.1.2",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.31",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "lighthouse": "^11.4.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
